<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(service) -%>">
  <span class='title'>
    <%=h service.title %>
    <span class="preview">&nbsp;</span>
  </span>
  <span class='actions'>
    <%= link_to refinery_icon_tag("application_go.png"), service_url(service),
        :title => t('.view_live'),
        :target => "_blank" %>
    <%= link_to refinery_icon_tag("application_edit.png"), edit_admin_service_path(service),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), admin_service_path(service),
        :class => "cancel confirm-delete",
        :title => t('.delete') %>
  </span>
</li>
